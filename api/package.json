{
  "name": "api",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "start": "npm run build && node dist/index.js",
    "dev": "nodemon src/app.ts",
    "prisma:setup": "npx prisma init --datasource-provider postgresql --output src/schema/generated/prisma",
    "prisma:merge": "npx prismerge -i src/prisma/prismerge.json",
    "prisma:migrate": "npx prisma migrate dev --schema=./src/prisma/schema/main.prisma",
    "prisma:generate": "prisma generate --schema=./src/prisma/schema/main.prisma",
    "prisma:pull": "npx prisma db pull --schema=./src/prisma/schema/main.prisma",
    "prisma:studio": "npx prisma studio --schema=./src/prisma/schema/main.prisma"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "module",
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.6",
    "@types/morgan": "^1.9.10",
    "@types/node": "^25.0.9",
    "@types/passport": "^1.0.17",
    "nodemon": "^3.1.11",
    "ts-node": "^1.7.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "@prisma-utils/prismerge": "^1.7.0",
    "@prisma/adapter-pg": "^7.2.0",
    "@prisma/client": "^7.2.0",
    "@types/cookie-parser": "^1.4.10",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^5.2.1",
    "morgan": "^1.10.1",
    "passport": "^0.7.0",
    "passport-google-oauth20": "^2.0.0",
    "passport-local": "^1.0.0",
    "prisma": "^7.2.0"
  }
}
